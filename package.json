    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.0.0",
    "gatsby-plugin-manifest": "^5.13.0",
    "gatsby-plugin-mdx": "^5.13.0",
    "gatsby-plugin-offline": "^6.13.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-source-filesystem": "^5.13.0",
    "npm": "^10.2.5",
    "prism-react-renderer": "^1.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
