{
  "name": "website",
  "version": "1.0.0",
  "description": "A personal website and blog about software development, science, and other musings by George Pearson.",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,mdx}\"",
